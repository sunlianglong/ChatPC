<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Client.Revert"
	x:Name="UserControl"
	d:DesignWidth="11" d:DesignHeight="12">

	<Grid x:Name="LayoutRoot">
		<Border Margin="2,0,0,3"  Background="{x:Null}" BorderBrush="#FF000000" BorderThickness="1,2,1,1" Width="9" Height="8"/>
		<Border Margin="-1,3,3,0" BorderBrush="#FF000000" BorderThickness="1,2,1,1" Height="8" Width="9"/>
	</Grid>
</UserControl>